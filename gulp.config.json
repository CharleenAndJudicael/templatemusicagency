{
    "css": "source/assets/css/*.css",
    "html": [
        "./source/*.html"
    ],
    "img": "./source/assets/img/*",
    "translate": "./node_modules/angular-translate/dist/angular-translate.min.js",
    "libsCss": [
        "./node_modules/bootstrap/dist/**/*"
    ],
    "browserifyPath": "./source/app.js",
    "js": [
        "./source/js/*.js"

    ],
    "public": "./public/",
